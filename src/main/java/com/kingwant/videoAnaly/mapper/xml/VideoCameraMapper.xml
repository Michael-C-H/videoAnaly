<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kingwant.videoAnaly.mapper.VideoCameraMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kingwant.videoAnaly.entity.VideoCamera">
        <id column="ID" property="id" />
        <result column="CODE" property="code" />
        <result column="NAME" property="name" />
        <result column="SOURCE" property="source" />
        <result column="ANLTSIS_TYPE" property="anltsisType" />
        <result column="ONLINE_NUM" property="onlineNum" />
        <result column="TOTAL" property="total" />
        <result column="LINE_PT1" property="linePt1" />
        <result column="LINE_PT2" property="linePt2" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID AS id, CODE AS code, NAME AS name, SOURCE AS source, ANLTSIS_TYPE AS anltsisType, ONLINE_NUM AS onlineNum, TOTAL AS total, LINE_PT1 AS linePt1, LINE_PT2 AS linePt2
    </sql>

</mapper>
